#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x, j = 1, a = 1, b = 1;
    
    printf("Digite o inteiro: ");
    scanf("%d", &x);
    
    while(j <= x) {
        while(a > 0) {
            printf("%d", j);
            a--;
            j++;
        }
        printf("\n");
        a = b + 1;
        b++;
    }
    return 0;
}
