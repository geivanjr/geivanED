#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i;
    int x[10] = {12, 32, -23, 63, 10, 85, 16, 756, -43656735, 90};
    
    printf("Imprimindo os inteiros por ponteiro\n\n");
    
    for(i = 9; i > 0; i--) {
        printf("%d, ", x[i]);
    }

    printf("%d.", x[0]);

    return 0;
}
